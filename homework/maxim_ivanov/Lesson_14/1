SELECT m.value  from marks m join students s on m.student_id = s.id
WHERE s.id = 835;

SELECT * from books b join students s on b.taken_by_student_id = s.id
WHERE s.id = 835;

SELECT * from students s
join marks m on s.id = m.student_id
join books b on s.id = b.taken_by_student_id
WHERE  s.id = 835;